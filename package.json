{
  "name": "vue-aurus",
  "version": "0.0.3",
  "private": true,
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/fojia/vue-aurus/blob/master/LICENSE.md"
    }
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/fojia/vue-aurus"
  },
  "author": {
    "name": "Vladimir Chairkin",
    "url": "https://github.com/fojia"
  },
  "keywords": [
        "vue-aurus",
        "dataviz",
        "vue",
        "d3",
        "charts",
        "swarmplot",
        "sunburst",
        "bar"
    ],
  "main": "dist/vue-aurus.umd.min.js",
  "scripts": {
